package VariablesAssignment;

public class YourSchedule 
{
	public static void main(String [] args)
	{
		String fteacher, fclass, steacher, sclass, tteacher, tclass, frteacher, frclass;
		
		fteacher="Mikkonen";
		fclass="PE/Health";
		steacher="Mauro";
		sclass="Computer Science";
		tteacher="Thow";
		tclass="English";
		frteacher="Incze";
		frclass="French";
		
		System.out.println("------------------------------------------------");
		System.out.println("| Per 1   |   " +fteacher+ "   |   " +fclass+ "        |");
		System.out.println("| Per 2   |   " +steacher+ "      |   " +sclass+ " |");
		System.out.println("| Per 3   |   " +tteacher+ "       |   " +tclass+ "          |");
		System.out.println("| Per 4   |   " +frteacher+ "      |   " +frclass+ "           |");
		System.out.println("------------------------------------------------");
		
	}

}
