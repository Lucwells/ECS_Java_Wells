import java.util.Scanner;
public class ALittleQuiz
{
	public static void main(String [] args)
	{
		Scanner keyboard = new Scanner(System.in);
		int capital, yesno, math;
		String apple;
		apple="''Apple''";
		System.out.println("Q1) What is the capital of California?");
		System.out.println("    1) San Francisco");
		System.out.println("    2) Sacramento");
		System.out.println("    3) San Diego");
		System.out.println(">");
		capital=keyboard.nextInt();
		if(capital==2)
		{
			System.out.println("Corrrect!");
		}
		else
		{
			System.out.println("That's wrong!");
		}
		System.out.println("");
		System.out.println("Q2) Can you store the value of " +apple+ " in a variable type int?");
	}
}
